import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        int f1 = 1, f2 = 1;
        
        for(int i=1; i<=a; i++)
        {
            f1 *= i;
        }
        
        for(int i=1; i<=b; i++)
        {
            f2 *= i;
        }
        
        System.out.println(f1-f2);
    }
}
